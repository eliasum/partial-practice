<Window x:Class="WindowsTracker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Главное окно" Height="350" Width="525">
    <Grid>
        <Button Name="cmdCreate"
                Height="22"
                Margin="22,-23,22,0"
                Content="Создать документ"
                Click="cmdCreate_Click" />

        <Button Name="cmdUpdate"
                Height="22"
                Margin="22,23,22,0"
                Content="Обновить документы"
                Click="cmdUpdate_Click" />
    </Grid>
</Window>
